{
	"name": "consistence/consistence-doctrine",
	"description": "Integration of Consistence library with Doctrine ORM",
	"license": "MIT",
	"authors": [
		{
			"name": "Va≈°ek Purchart",
			"email": "me@vasekpurchart.cz",
			"homepage": "http://vasekpurchart.cz"
		}
	],
	"require": {
		"consistence/consistence": "~1.0",
		"doctrine/annotations": "~1.2",
		"doctrine/orm": "~2.5.0"
	},
	"require-dev": {
		"consistence/coding-standard": "1.0",
		"jakub-onderka/php-console-highlighter": "0.3.2",
		"jakub-onderka/php-parallel-lint": "0.9.2",
		"phing/phing": "2.16",
		"phpunit/phpunit": "5.7.5"
	},
	"autoload": {
		"psr-4": { "Consistence\\Doctrine\\": [ "src" ] },
		"classmap": [ "src" ]
	},
	"autoload-dev": {
		"psr-4": { "Consistence\\Doctrine\\": [ "tests" ] },
		"classmap": [ "tests" ]
	},
	"config": {
		"bin-dir": "bin"
	}
}
